	        "selectedIconPath": "./static/images/tabs/icon_category_active@3x.png",
	        "text": "分类"
	    },{
	        "pagePath": "pages/cart/cart",
	        "iconPath": "./static/images/tabs/icon_cart@3x.png",
	        "selectedIconPath": "./static/images/tabs/icon_cart_active@3x.png",
	        "text": "购物车"
	    }, {
	        "pagePath": "pages/me/me",
	        "iconPath": "./static/images/tabs/icon_me@3x.png",
	        "selectedIconPath": "./static/images/tabs/icon_me_active@3x.png",
	        "text": "我的"
	    }]
